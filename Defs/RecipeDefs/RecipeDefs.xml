<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<!---->
	<RecipeDef>
		<defName>Sift_GatheredPlant_CraftingSpot</defName>
		<label>sift gathered plant</label>
		<description>Sift through gathered plants for useful material like wood and berries. It's not very effective, but a good way to dispose of gathered plant.</description>
		<jobString>sifting through gathered plants.</jobString>
		<workSpeedStat>GeneralLaborSpeed</workSpeedStat>
		<effectWorking>Cook</effectWorking>
		<soundWorking>Recipe_Tailor</soundWorking>
		<workAmount>50</workAmount>
		<workSkillLearnFactor>0.25</workSkillLearnFactor>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>GatheredPlant</li>
					</thingDefs>
				</filter>
				<count>10</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>GatheredPlant</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<WoodLog>1</WoodLog>
			<RawBerries>1</RawBerries>
		</products>
		<workSkill>Crafting</workSkill>
		<recipeUsers>
			<li>CraftingSpot</li>
		</recipeUsers>
	</RecipeDef>
	<!---->
	<RecipeDef>
		<defName>Make_PlantFiber_FromGatheredPlant_CraftingSpot</defName>
		<label>make plant fiber by hand</label>
		<description>Make plant fiber by hand at a crafting spot. It's not very effective, you really could use a tailoring bench...</description>
		<jobString>Making plant fiber.</jobString>
		<workSpeedStat>GeneralLaborSpeed</workSpeedStat>
		<effectWorking>Cook</effectWorking>
		<soundWorking>Recipe_Tailor</soundWorking>
		<workAmount>750</workAmount>
		<workSkillLearnFactor>0.25</workSkillLearnFactor>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>GatheredPlant</li>
					</thingDefs>
				</filter>
				<count>10</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>GatheredPlant</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<PlantFibber>5</PlantFibber>
		</products>
		<workSkill>Crafting</workSkill>
		<recipeUsers>
			<li>CraftingSpot</li>
		</recipeUsers>
	</RecipeDef>
